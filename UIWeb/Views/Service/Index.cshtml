         <div class="w-100 float-left service-box-con">
            <div class="container">
               <div class="service-box-inner-con">
                  <div class="row ml-0 mr-0">
                    
                   @foreach (var item in Model.Services)
                   {
                        <div class="col-lg-3 col-md-3 col-sm-6  pl-0 pr-0">
                     <div class="feature-box-item text-center ">
                        <figure>
                            <i class= "@item.IconUrl" style = "font-size: 40px; "></i>
                          
                        </figure>
                        <h5>@item.ServiceName</h5>
                        <p class="mb-0">@item.ServDescription</p>
                     </div>
                  </div>
                   }
                
                  </div>
               </div>
             
             
            </div>
         </div>
      </section>
